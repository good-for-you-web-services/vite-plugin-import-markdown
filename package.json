{
	"name": "@goodforyou/vite-plugin-markdown-import",
	"description": "Vite plugin to import markdown files with support for front-matter",
	"private": false,
	"version": "1.2.2",
	"repository": {
		"type": "git",
		"url": "https://github.com/good-for-you-web-services/vite-plugin-import-markdown"
	},
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./client": {
			"types": "./client.d.ts"
		}
	},
	"scripts": {
		"dev": "tsc --watch",
		"build": "tsc",
		"prepare": "npm run build"
	},
	"files": [
		"dist",
		"client.d.ts"
	],
	"keywords": [
		"vite",
		"vite-plugin",
		"markdown",
		"front-matter"
	],
	"devDependencies": {
		"@types/node": "^22.8.6",
		"typescript": "^5.6.3"
	},
	"peerDependencies": {
		"front-matter": "^4.0.0",
		"vite": "^6.0.0"
	}
}
